<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>delivery</display-name>
  <welcome-file-list>
    <welcome-file>delivery.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>UserFrontController</servlet-name>
    <servlet-class>com.delivery.app.user.UserFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserFrontController</servlet-name>
    <url-pattern>*.us</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StoreFrontController</servlet-name>
    <servlet-class>com.delivery.app.store.StoreFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StoreFrontController</servlet-name>
    <url-pattern>*.st</url-pattern>
  </servlet-mapping>
</web-app>